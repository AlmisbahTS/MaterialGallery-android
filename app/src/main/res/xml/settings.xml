<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <SwitchPreference
        android:defaultValue="false"
        android:key="is_dark_theme"
        android:title="Dark Theme" />
    <ListPreference
        android:defaultValue="rounded"
        android:entries="@array/shape_theme"
        android:entryValues="@array/shape_theme_value"
        android:key="shape_theme"
        android:title="Shape Theme" />
    <PreferenceScreen
        android:key="material_components_version"
        android:title="Material Components Version" />
</PreferenceScreen>